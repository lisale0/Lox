fun strAppend(str1, str2) {
  var str = str1 + str2;
  return str;
}

fun add(a, b){
  return(a + b);
}

fun checkNegative(num){
  if(num <= 0)
    return(true);
  else
    return(false);
}

fun isOdd(n) {
  if (n == 0) return false;
  return isEven(n - 1);
}

fun isEven(n){
  if (n == 0) return true;
  return isOdd(n - 1);
}

print(checkNegative(-1));
print(add(10, 5));
print(strAppend("foo", "bar"));
print(isEven(3));